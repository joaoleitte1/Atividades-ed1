#include <stdlib.h>
#include <time.h>
#include <stdio.h>

int main(){
	
	int v[5];
	int aux;
	
	srand(time(NULL));
	
	for (int i = 0; i < 5; i++) {
		v[i] = 1+rand()%10;
		printf("%d\n", v[i]);
	}
	
	
	for (int cont = 0; cont < 5-1; cont++) {
	  for (int i = 0; i < 5 - 1 - cont ; i++) {	 
		 if (v[i] > v[i+1]) {
			 aux = v[i];
			 v[i] = v[i+1];
			 v[i+1] = aux;		 
		  }
	  
	  }

	}
	 
	
	printf("\n");
	
	for (int i = 0; i < 5; i++) {
		printf ("%d\n", v[i]);
	}
	
	return 0;
}

